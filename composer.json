{
    "name": "clickpdx/devenv",
    "description": "Development Environment for WordPress",
    "type": "project",
    "authors": [
        {
            "name": "Trevor",
            "email": "brainard@my.lanecc.edu"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require-dev": {
        "wpackagist-plugin/custom-sidebars":"dev-trunk",
        "wpackagist-plugin/custom-post-widget":"dev-trunk",
        "wpackagist-plugin/wp-media-manager-lite":"dev-trunk"
    },
    "extra":{
        "installer-paths":{
            "wp-content/plugins/custom-sidebars":["wpackagist-plugin/custom-sidebars"],
            "wp-content/plugins/custom-post-widget":["wpackagist-plugin/custom-post-widget"],
            "wp-content/plugins/wp-media-manager-lite":["wpackagist-plugin/wp-media-manager-lite"]
        }   
    },
    "require":{
        "composer/installers": "~1.0"
    }
    
}

{
    "name": "clickpdx/devenv",
    "description": "Development Environment for WordPress",
    "type": "project",
    "authors": [
        {
            "name": "jack & trevor",
            "email": "brainard@my.lanecc.edu"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require-dev": {
        "wpackagist-plugin/my-social-widget":"dev-trunk",
        "clickpdx/wireframe": "dev-master",
        "clickpdx/oadp-wordpress-theme": "dev-master"
    },
    "extra":{
        "installer-paths":{
            "wordpress/wp-content/plugins/my-social-widget": ["wpackagist-plugin/my-social-widget"],
            "wordpress/wp-content/themes/wireframe": ["clickpdx/wireframe"],
            "wordpress/wp-content/themes/oadp": ["clickpdx/oadp-wordpress-theme"]
        }
    }
}
